  <style type="text/css">
          .unique-{{ section.id}} .section-cta {
            {%if section.settings.buttontxtcolor %}color:{{section.settings.buttontxtcolor}} !important; {%endif%}
            {%if section.settings.buttonborder %} border:1px solid {{section.settings.buttonborder}} !important;{%endif%}
            {%if section.settings.buttonbgcolor %} background-color:{{section.settings.buttonbgcolor}} !important;{%endif%}
          }
    	  .unique-{{ section.id}} .section-cta:hover{
             {%if section.settings.button_mouseover_txt_color %}color:{{section.settings.button_mouseover_txt_color}} !important; {%endif%}
             {%if section.settings.button_mouseover_color %} background-color:{{section.settings.button_mouseover_color}} !important;{%endif%}
             {%if section.settings.button_mouseover_brdr_color %} border:1px solid {{section.settings.button_mouseover_brdr_color}} !important;{%endif%}
          }
    	.unique-{{ section.id}} p {
        	{%if section.settings.description_color %}color:{{section.settings.description_color}} !important; {%endif%}
        }
