    font-style: normal;
}
@font-face {
    font-family: 'nimbus_sans_lbold';
    src: url("{{'NimbusSanL-Bol-webfont.eot' | asset_url}}");
    src: url("{{'NimbusSanL-Bol-webfont.eot?#iefix' | asset_url}}") format('embedded-opentype'),
         url("{{'NimbusSanL-Bol-webfont.woff2' | asset_url}}") format('woff2'),
         url("{{'NimbusSanL-Bol-webfont.woff' | asset_url}}") format('woff'),
