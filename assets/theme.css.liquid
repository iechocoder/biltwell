  font-style: normal;
  font-stretch: normal;
  line-height: 24px;
  position: absolute;
  right: 7%;
  bottom: -3px;
  letter-spacing: normal;
  color: #2b2b2b;
  z-index: 1;
  opacity: .65;
  transition: opacity 300ms ease;
}
.product-description.has-more.expanded::before{
  opacity: 0;
}
.product-description.has-more::after {
  content: "";
  text-align: right;
  position: absolute;
  bottom: 0;
  right: 0;
  width: 70%;
  height: 1.2em;
  background: linear-gradient(to right, rgba(255, 255, 255, 0), rgba(255, 255, 255, 1) 50%);
}

