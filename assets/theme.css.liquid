  float: right;
}
.chocolat-wrapper .chocolat-description{
  display: inline-block;
  float: left;
}

/* no container mode*/
body.chocolat-open>.chocolat-overlay{
  z-index:15;
}
body.chocolat-open>.chocolat-loader{
  z-index:15;
}
body.chocolat-open>.chocolat-content{
  z-index:17;
}

