@import url("font-awesome.css");

@font-face {
  font-family: 'helvetica_bold';
  src: url("{{'helvetica_neue_ce_75_bold-webfont.woff2' | asset_url}}") format('woff2'),
    url("{{'helvetica_neue_ce_75_bold-webfont.woff' | asset_url}}") format('woff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
