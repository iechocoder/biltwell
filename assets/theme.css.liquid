         url("{{'NimbusSanL-RegIta-webfont.woff2' | asset_url}}") format('woff2'),
         url("{{'NimbusSanL-RegIta-webfont.woff' | asset_url}}") format('woff'),
         url("{{'NimbusSanL-RegIta-webfont.ttf' | asset_url}}") format('truetype'),
