body {
  background-color: #fff;
  color: #979797;
  font-size: 18px;
  line-height: 22px;
  font-family: "Helvetica Neue", "nimbus_sans_lregular", Helvetica, Arial, "Lucida Grande", sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-smoothing: antialiased;
}

body,
#page {
  overflow-x: hidden !important;
}

h1,
h2,
h3,
h4,
h5,
h6,
ul,
li,
p {
  margin: 0;
  padding: 0;
}

a:hover {
  transition: all 300ms;
  text-decoration: none;
}

a:focus,
button:focus,
input:focus {
  outline: none !important;
  box-shadow: none !important;
}

.borderbottom {
  border-bottom: 1px solid #f2f2f2;
}

.gridbox-title {
  font-size: 72px;
  color: #2b2b2b;
  font-family: "Helvetica Neue", "nimbus_sans_lbold", Helvetica, Arial, "Lucida Grande", sans-serif;
  line-height: 0.94;
  letter-spacing: -0.5px;
  font-weight: 600;
}

.addmargin {
  margin: 20px 0;
}

.nomargin {
  margin: 0;
}

.showonmobile-left {
  display: none;
}

.showonmobile-right {
  display: none;
}

.gridbox-desc {
  font-size: 18px;
  line-height: 1.33;
  letter-spacing: normal;
  color: #2b2b2b;
  margin-bottom: 25px;
  opacity: 0.5;
}

.gridbox-link {
  display: flex;
  justify-content: center;
  width: 155px;
  font-size: 16px;
  line-height: 44px;
  letter-spacing: -0.1px;
  color: #c4161d;
  border: 1px solid #c4161d;
  font-weight: 500;
}

.gridbox-link:hover {
  background: #c4161d;
  color: #fff;
}

.footer .container,
.bestseller .container {
  max-width: 1595px;
}

.grid-thumbnail {
  width: 100%;
}

/* header starts here ------------------- */
.header .container {
  max-width: 1595px;
}

.header-showroom-row {
  background-color: #ba121a;

}

.header-showroom-row p {
  color: #fff;
  font-size: 16px;
  line-height: 1.1;
  font-weight: normal;
  width: 100%;
  text-align: center;
  padding: 13px 0;
}
.header-showroom-row p a{
  color: #fff;
  text-decoration: underline;
}

.header-shipping-row {
  background-color: #131313;
  padding: 8px 0;
}

.header-shipping-row .row {
  justify-content: space-between;
}

.header-shipping-row span {
  display: inline-block;
  color: #fff;
  font-size: 12px;
  line-height: 1.8;
  margin-left: 10px;
  letter-spacing: 1.7px;
}

.header-shipping-row .header-shipping-list {
  display: flex;
  justify-content: flex-end;
  flex-wrap: wrap;
}

.header-shipping-row ul {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  width: 390px;
}

.header-shipping-row ul li {
  display: flex;
  margin-right: 40px;
}

.header-shipping-row ul li a:hover {
  color: #ba121a;
}

.header-shipping-row span,
.header-shipping-row svg,
.header-shipping-row img {
  display: inline-block;
  vertical-align: middle;
}

.header-shipping-row svg,
.header-shipping-row img {
  max-height: 16px;
}
.header-shipping-row img {
  max-height: initial;
  max-width: 24px;
  margin: 0;
}
.header-shipping-row img + span{
  margin-left: .5rem;
  font-weight: 500;
}

.header-shipping-row ul li:first-child {
  margin-right: 8px;
}

.header-shipping-row ul li:last-child {
  margin-right: 0;
}

.header-shipping-row ul li a {
  color: #fff;
  font-size: 12px;
  text-transform: uppercase;
  letter-spacing: 1.7px;
}

.main-header {
  padding: 0;
  position: relative;
  border-bottom: 1px solid #ecebeb;
}

.main-header .navbar {
  padding: 0;
  position: static;
}

.main-header .navbar-brand {
  padding: 0;
}

.main-header .navbar-nav li {
  margin: 0 12px;
  padding: 0 3px;
  position: inherit;
}
.main-header .navbar-nav li {
  margin: 0 0;
  padding: 0 15px;
}

.main-header .navbar-nav:first-child li:first-child {
  margin-left: 100px;
}

.main-header .navbar-nav li:last-child {
  margin-right: 0;
}

.main-header .navbar .nav-link {
  display: block;
  padding: 38px 0;
  font-size: 14px;
  line-height: 1.71;
  letter-spacing: 0.9px;
  color: #131313;
  text-transform: uppercase;
  text-align: center;
  border-bottom: 4px solid transparent;
}

.main-header .navbar .nav-link:hover,
.main-header .navbar .nav-item:hover>.nav-link {
  color: #a1a1a1;
}

.main-header .navbar .dropdown .nav-link:hover,
.main-header .navbar .dropdown:hover>.nav-link {
  border-color: #131313;
}

.dropdown-menu {
  border: none;
}

#videosection .video-cont {
  line-height: 0;
}

.navbar-light .navbar-nav .active>.nav-link,
.navbar-light .navbar-nav .nav-link.active,
.navbar-light .navbar-nav .nav-link.show,
.navbar-light .navbar-nav .show>.nav-link {
  opacity: 0.5;
}

.main-header .container,
.main-header .container .row,
.main-header .container .row>div {
  position: static;
}

.main-header .navbar-nav .dropdown-menu {
  width: 100%;
  left: 0;
  width: 100%;
  top: 99%;
  border-radius: 0;
}

.main-header .nav-helmet-row {
  padding: 25px 0;
  display: flex;
  justify-content: space-between;
  max-width: 1595px;
  margin: 0 auto;
}

.main-header .nav-helmet-row li {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  border-radius: 3px;
  padding: 20px 15px;
  margin: 0 !important;
}

.main-header .nav-helmet-row li a {
  text-align: center;
}
.main-header .nav-helmet-row li a:hover {
 opacity: 0.5;
}
.main-header .nav-helmet-row li img {
  object-fit: cover;
  max-width: 100%;
  height: auto;
}
.main-header .nav-helmet-row li img {
  object-fit: initial;
  max-width: 200px;
  width: 100%;
}

.main-header .nav-helmet-row li span {
  width: 100%;
  text-align: center;
  color: #131313;
  font-size: 16px;
  font-weight: 500;
  display: inline-block;
  width: 100%;
  margin: 30px 0 0 0;
}

/* .main-header .nav-helmet-row li:hover { background-color: #F5F5F5;}*/
.main-header .navbar-nav li .submenu-outer {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  max-width: 1595px;
  margin: 0 auto;
}

.main-header .navbar-nav li .sub-menu {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 25px 0;
  margin: 0 30px;
  width: 18%;
  align-items: flex-start;
}

.main-header .navbar-nav li .sub-menu li {
  list-style-type: none;
  margin: 0 !important;
  width: 100%;
  padding: 0 0 8px;
}

.main-header .navbar-nav li .sub-menu li h6 {
  color: #131313;
  opacity: 0.4;
  font-size: 12px;
  margin: 0 0 15px 0;
  padding: 0;
  text-transform: uppercase;
  justify-content: flex-start;
  letter-spacing: 1.71px;
  line-height: 32px;
  font-family: "Helvetica Neue", "nimbus_sans_lbold", Helvetica, Arial, "Lucida Grande", sans-serif;
  font-weight: 600;
}

.main-header .navbar-nav li .sub-menu li a {
  font-size: 16px;
  line-height: 1.71;
  letter-spacing: 0.9px;
  color: #131313;
  opacity: 1;
}
.main-header .navbar-nav li .sub-menu li a:hover{
  opacity:0.4;
}
.nav-item a svg {
  vertical-align: middle;
}

@media (max-width:1699px) {
  .main-header .nav-helmet-row {
    max-width: inherit;
    padding: 25px 100px;
  }

  .main-header .navbar-nav li .submenu-outer {
    max-width: inherit;
    padding: 0 100px;
  }
}

/* header ends here ------------------------------- */

/* banner starts here ----------------------------- */
