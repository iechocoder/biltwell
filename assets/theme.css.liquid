.video-main {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 5;
  height: 100%;
  background: #fff;
}
@media screen and (max-width: 999px){
  .product .play-video {
      z-index: 9999;
      max-width: 200px;
      position: relative;
      margin: 1rem auto;
      right: initial;
      left: initial;
  }
}
.embed-container {
  position: relative;
  padding-bottom: 56.25%;
  height: 0;
  overflow: hidden;
  max-width: 100%;
}
.video-main .embed-container{
  transform: perspective(1px) translateY(-33%) translateZ(0);
  top: 33%;
  backface-visibility: hidden;
  -webkit-font-smoothing: subpixel-antialiased;
}
.embed-container iframe, .embed-container object, .embed-container embed {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
